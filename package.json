{
    "name": "roosterjs",
    "version": "6.19.2",
    "description": "Framework-independent javascript editor",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/roosterjs"
    },
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist",
        "copysample": "node tools/copy-sample-code.js",
        "dts": "node tools/dts.js",
        "tslint": "node node_modules/tslint/bin/tslint --project tools/tsconfig.tslint.json",
        "normalize": "node tools/normalize-packages.js",
        "pack": "node tools/pack.js && node tools/pack.js -amd -e",
        "pack:prod": "node tools/pack.js -p && node tools/pack.js -p -amd -e",
        "b": "npm run clean && npm run normalize && npm run tslint && node tools/build-all.js",
        "b:prod": "npm run clean && npm run normalize && npm run tslint && node tools/build-all.js -p",
        "builddemo": "node tools/packDemo.js",
        "build": "npm run b:prod && npm run pack && npm run pack:prod && npm run dts && npm run copysample && npm run builddemo",
        "start": "webpack-dev-server",
        "test": "karma start",
        "test:debug": "karma start --no-single-run",
        "prettier": "node tools/prettier.js",
        "publish": "node tools/publish-all.js"
    },
    "devDependencies": {
        "@types/jasmine": "2.6.0",
        "glob": "7.1.2",
        "jasmine-core": "2.8.0",
        "karma": "2.0.0",
        "karma-chrome-launcher": "2.2.0",
        "karma-jasmine": "1.1.1",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-sourcemap-loader": "0.3.7",
        "karma-webpack": "3.0.0",
        "ncp": "2.0.0",
        "prettier": "1.15.3",
        "rimraf": "2.6.2",
        "toposort": "1.0.4",
        "ts-loader": "^5.3.1",
        "tslint": "5.11.0",
        "tslint-eslint-rules": "5.4.0",
        "tslint-microsoft-contrib": "5.2.1",
        "typescript": "^3.2.2",
        "webpack": "^4.27.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10",
        "//": "Dependencies below are for sample site only",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "@microsoft/loader-load-themed-styles": "^1.7.118",
        "@types/node": "^10.12.15",
        "@types/object-assign": "4.0.30",
        "@types/react": "^16.7.17",
        "@types/react-dom": "^16.0.11",
        "css-loader": "^2.0.1",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "url-loader": "^1.1.2"
    }
}
