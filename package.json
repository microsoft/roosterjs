{
  "name": "roosterjs",
  "version": "6.1.0",
  "description": "Framework-independent javascript editor",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/roosterjs"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "tslint": "node node_modules/tslint/bin/tslint --project tools/tsconfig.tslint.json",
    "normalize": "node tools/normalize-packages.js",
    "pack": "node tools/pack.js",
    "pack:prod": "node tools/pack.js -p",
    "build": "npm run clean && npm run normalize && npm run tslint && node tools/build-all.js && npm run pack",
    "start": "webpack-dev-server",
    "test": "npm run build && karma start",
    "test:debug": "npm run build && karma start --no-single-run"
  },
  "devDependencies": {
    "@types/jasmine": "2.6.0",
    "jasmine-core": "2.8.0",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.4",
    "rimraf": "2.6.2",
    "toposort": "1.0.4",
    "ts-loader": "2.3.7",
    "tslint": "5.7.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-microsoft-contrib": "5.0.1",
    "typescript": "2.5.3",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.1"
  }
}
